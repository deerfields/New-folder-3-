# Basic configuration for development
NODE_ENV=development
PORT=3001
LOG_LEVEL=debug

# JWT Secret (for development only)
JWT_SECRET=dev-jwt-secret-key

# CORS
CORS_ORIGIN=http://localhost:3000

# Feature flags - disable complex services for development
ENABLE_AI_ANALYTICS=false
ENABLE_IOT_INTEGRATION=false
ENABLE_BLOCKCHAIN=false
ENABLE_COMPUTER_VISION=true
ENABLE_PREDICTIVE_MAINTENANCE=false
ENABLE_FACIAL_RECOGNITION=false
ENABLE_VOICE_ASSISTANT=false
ENABLE_AR_VR=false
